        "eslint-config-standard": "17.0.0",
        "eslint-plugin-chai-friendly": "0.7.2",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-promise": "6.1.1",
        "mocha": "10.2.0",
        "nodemon": "2.0.20",
        "prettier": "2.8.3",
        "sequelize-cli": "6.5.2"
